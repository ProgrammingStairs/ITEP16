<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h2>Example of Directive Tag</h2>
	<form action="numberPage.jsp">
		Enter First Number <br>
		<input
			type="text"
			placeholder="Enter first number"
			name="num1"
			id="num1"
		><br>
		Enter Second Number <br>
		<input
			type="text"
			placeholder="Enter second number"
			name="num2"
			id="num2"
		><br>
		<input
			type="submit"
			value="Submit"
		><input
			type="reset"
			value="Reset"
		>
		<br>
	</form>
</body>
</html>